{
  "name": "sog-page-builder",
  "version": "0.0.1",
  "description": "Page Builder Tool",
  "main": "dist/main.js",
  "bugs": {
    "url": "https://github.com/slaveofgod/sog-page-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slaveofgod/sog-page-builder.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:js": "",
    "build:css": "node-sass --watch \"media\" -o \"media\" ",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "concurrently --handle-input \"tsc -w -p tsconfig.build.json\" \"npm run server\" ",
    "server": "light-server -s . -p 9090 -w \"build/**/*.js, ./index.html # npm run build:js\" -w \"media/**/*.css # npm run build:css # reloadcss\"",
    "lint": "tslint -p __tsconfig.json -c tslint.json",
    "test": "jest  --env=node",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "page",
    "builder"
  ],
  "author": {
    "name": "Slave of God",
    "email": "iamtheslaveofgod@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cache-manager": "^2.10.3",
    "@types/correlation-id": "^2.1.0",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.6",
    "@types/html-minifier": "^3.5.3",
    "@types/intercom-client": "^2.11.8",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.156",
    "@types/node": "^14.0.13",
    "@types/supertest": "^2.0.9",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "light-server": "^2.7.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cache-manager": "^3.3.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cli-progress": "^3.8.2",
    "commander": "^5.1.0",
    "concurrently": "^5.2.0",
    "correlation-id": "^3.1.1",
    "date-fns": "^2.14.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "html-minifier": "^4.0.0",
    "intercom-client": "^2.11.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "pg": "^8.2.1",
    "pretty-error": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.8.0",
    "url-join": "^4.0.1"
  }
}
